package com.j2.command.dvd;

public class DvdInsertNextCommand implements Command{
  private DvdBox dvdbox;
  private Status lastPlay;
  public DvdPlayCommand(DvdBox dvdbox){
    this.dvdbox = dvdbox;
  }
  public void execute(){
    lastPlay = dvdbox.getPlay();
    dvdbox.playNext();
    dvdbox.play();
  }
  public void undo(){
    int temp = dvdbox.getPlay();
    dvdbox.playIndex(lastPlay);
    lastPlay = temp;
    dvdbox.play();
  }
}